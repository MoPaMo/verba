datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id           String   @id @default(uuid())
    email        String   @unique
    password     String
    name         String?
    profileEmoji String?  @default("ðŸ‘¤")
    createdAt    DateTime @default(now())
}
